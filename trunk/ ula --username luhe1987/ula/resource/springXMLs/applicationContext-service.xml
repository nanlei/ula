<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans-2.0.dtd">
<beans default-autowire="byName" default-merge="true">

	<bean id="logService" parent="baseTxService">
		<property name="target">
			<bean class="ula.service.admin.LogService" />
		</property>
	</bean>
	
	<bean id="aboutService" parent="baseTxService">
		<property name="target">
			<bean class="ula.service.admin.AboutService" />
		</property>
	</bean>
	
	<bean id="contactService" parent="baseTxService">
		<property name="target">
			<bean class="ula.service.admin.ContactService" />
		</property>
	</bean>
	<bean id="cityServcie" parent="baseTxService">
		<property name="target">
			<bean class="ula.service.admin.CityServcie" />
		</property>
	</bean>

	<bean id="serviceManager" class="ula.common.ServiceManager" />
	
</beans>



